@{
    ViewBag.Title = "Que Ads.co.za - My Ads";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section styles{
    <link href="@Url.Content("~/Content/local/viewads.css")" rel="stylesheet">
}

@section JsCode{
    angular.bootstrap(document.getElementById('divUserAdsContainer'), ['userAdsApp'])
}

@section scripts{
    <script src="@Url.Content("~/App/UserAds/js/app.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/App/UserAds/js/controllers/userads.js")" type="text/javascript"></script>
}

<div ng-non-bindable>
    <div id="divUserAdsContainer" class="container-fluid">
        <ng-view></ng-view>
    </div>
</div>
